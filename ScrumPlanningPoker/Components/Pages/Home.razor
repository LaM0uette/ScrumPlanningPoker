@page "/"
@attribute [RenderModeInteractiveServer(false)]
@inherits HomeBase

<h2>Create a New Room</h2>

<button @onclick="Create">Créer une Room</button>

<br/>

@if (!string.IsNullOrEmpty(RoomLink))
{
    <p>@RoomLink</p>
    
    <button>
        <a href="@RoomLink">Lien de la room</a>
    </button>
}
